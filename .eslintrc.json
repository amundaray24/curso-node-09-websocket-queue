{
  "env": {
    "es6": true,
    "browser": true,
    "amd": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
  },
  "ignorePatterns": [
    "node_modules", 
    "Dockerfile",
    "docker-compose.yml"
  ],
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  }
}